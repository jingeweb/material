{
  "name": "jinge-material-site",
  "version": "3.1.0",
  "description": "A material design ui library implemented with jinge mvvm framework",
  "main": "lib/index.js",
  "types": "src/index.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config ./scripts/webpack.config.js",
    "dev-build": "rm -rf ./docs && webpack --config ./scripts/webpack.config.js",
    "build": "export PROD=true && npm run build-theme && npm run dev-build",
    "build-theme": "webpack --config ./scripts/webpack.config.theme.js",
    "i18n:scan": "jinge-i18n extract --scan src --locale en",
    "i18n:gen": "jinge-i18n generate",
    "eslint": "eslint './{src,compiler,scripts}/**/*.{js,ts}' --fix",
    "stylelint": "stylelint './src/**/*.scss' --fix",
    "lint": "npm run eslint && npm run stylelint",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuhangGe/jinge-material.git"
  },
  "keywords": [
    "material",
    "ui",
    "library",
    "jinge"
  ],
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.scss": "stylelint --fix"
  },
  "author": "Yuhang Ge <abeyuhang@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YuhangGe/jinge-material/issues"
  },
  "homepage": "https://github.com/YuhangGe/jinge-material#readme",
  "peerDependencies": {
    "jinge": ">= 3.2.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "clean-css": "^5.3.1",
    "css-loader": "^6.7.1",
    "esbuild": "^0.14.50",
    "esbuild-loader": "^2.19.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-minifier-terser": "7.0.0-beta.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "clipboard": "^2.0.11",
    "jinge": "^3.2.2",
    "jinge-compiler": "^3.2.2",
    "jinge-i18n": "^3.2.1",
    "jinge-material": "^3.2.0",
    "jinge-material-icons": "^1.0.1",
    "jinge-router": "^3.2.2",
    "lodash-es": "^4.17.21",
    "prismjs": "^1.28.0"
  }
}
